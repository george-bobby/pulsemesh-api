{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"framework": "vite",
	"installCommand": "npm install",
	"devCommand": "npm run dev",
	"env": {
		"ROLLUP_NO_NATIVE": "1"
	},
	"build": {
		"env": {
			"ROLLUP_NO_NATIVE": "1"
		}
	},
	"routes": [
		{
			"src": "/assets/(.*)",
			"headers": {
				"cache-control": "max-age=31536000, immutable"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
